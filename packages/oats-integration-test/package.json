{
  "name": "@smartlyio/oats-integration-test",
  "version": "2.17.0",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": true,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate:test": "find test -name driver.ts -exec yarn ts-node -r tsconfig-paths/register --project tsconfig.json {} \\;",
    "generate:render": "yarn ts-node examples/driver.ts",
    "pretest": "yarn generate:render && yarn generate:test ",
    "clean": "rm -rf ./tmp && find test -name '*generated.ts' -exec rm {} \\;",
    "lint": "eslint --max-warnings=0 --ext .ts test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.17.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "openapi3-ts": "^2.0.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "2.17.0",
    "@smartlyio/oats": "2.17.0",
    "@smartlyio/oats-koa-adapter": "2.17.0",
    "@smartlyio/oats-runtime": "2.17.0",
    "@smartlyio/oats-fast-check": "2.17.0",
    "@types/jest": "27.0.1",
    "@types/js-yaml": "4.0.1",
    "@types/koa": "2.13.1",
    "@types/koa-mount": "4.0.0",
    "@types/koa-router": "7.4.2",
    "@types/lodash": "4.14.168",
    "@types/node": "14.17.0",
    "axios": "0.21.1",
    "form-data": "4.0.0",
    "jest": "27.0.3",
    "jsverify": "0.8.4",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-mount": "4.0.0",
    "koa-router": "10.0.0",
    "ts-jest": "27.0.1",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  }
}
