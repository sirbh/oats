{
  "name": "@smartlyio/oats",
  "version": "2.17.0",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn",
    "clean": "lerna exec --no-private --concurrency 4 -- yarn clean",
    "tsc": "tsc",
    "pretest": "lerna run pretest",
    "test": "jest --coverage --forceExit",
    "test:watch": "jest --watch",
    "lint": "lerna run lint",
    "render": "lerna run render",
    "build": "lerna run build",
    "ci:install": "yarn install --frozen-lockfile --link-duplicates --silent",
    "ci:lint": "yarn lint",
    "ci:test": "yarn test",
    "ci:build": "yarn run --silent build",
    "publish:canary": "yarn lerna publish --canary --no-private",
    "publish:patch": "yarn lerna publish patch --no-private",
    "publish:minor": "yarn lerna publish minor --no-private",
    "publish:major": "yarn lerna publish major --no-private"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "lerna": "^4.0.0",
    "ts-node": "10.2.1"
  },
  "dependencies": {
    "jest": "27",
    "ts-jest": "27.0.5",
    "typescript": "4.3.5",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.0",
    "@typescript-eslint/parser": "4.29.1",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "prettier": "2.3.2",
    "fast-check": "2.17.0"
  }
}
